{
  "compilerOptions": {
    "types": ["node", "prettier", "trusted-types"],
    "target": "ES6",
    "module": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "react-jsx",
    "skipDefaultLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "component/*": [
        "./component/*"
      ]
    },
    "noImplicitAny": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}